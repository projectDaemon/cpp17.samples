[project]
	description = New C++17 features presentation.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Egorov Andrew (projectDaemon)
[access "refs/for/refs/*"]
	read = group user/Egorov Andrew (projectDaemon)
	push = group user/Egorov Andrew (projectDaemon)
	label-Code-Review = -2..+2 group user/Egorov Andrew (projectDaemon)
	label-Verified = -1..+1 group user/Egorov Andrew (projectDaemon)
	rebase = group user/Egorov Andrew (projectDaemon)
	removeReviewer = group user/Egorov Andrew (projectDaemon)
	submit = group user/Egorov Andrew (projectDaemon)
[access "refs/heads/*"]
	read = group user/Egorov Andrew (projectDaemon)
	create = group user/Egorov Andrew (projectDaemon)
	push = +force group user/Egorov Andrew (projectDaemon)
	pushMerge = group user/Egorov Andrew (projectDaemon)
[access "refs/tags/*"]
	push = group user/Egorov Andrew (projectDaemon)
	push = +force group user/Egorov Andrew (projectDaemon)
